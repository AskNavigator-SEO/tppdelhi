RewriteEngine On

RewriteBase / 
RewriteCond %{HTTP_HOST} ^thepackersmoversdelhi\.com [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://www.thepackersmoversdelhi.com/$1 [R,L]

ErrorDocument 404 https://thepackersmoversdelhi.com/
Options All -Indexes

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^/]+)/$ $1.php
RewriteRule ^([^/]+)/([^/]+)/$ /$1/$2.php
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !(\.[a-zA-Z0-9]{1,5}|/)$
RewriteRule (.*)$ /$1/ [R=301,L]

AddHandler application/x-httpd-ea-php74 .html .htm

